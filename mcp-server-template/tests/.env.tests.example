# E2E Test Configuration
# Copy this file to .env.test and fill in your values

# Base URL of the running MCP weather server
MCP_WEATHER_TEST_BASE_URL="http://localhost:8110"

# Request timeout for E2E tests (seconds)
MCP_WEATHER_TEST_TIMEOUT_SECONDS=60

# Private key for x402 payment testing (wallet must have USDC on the configured network)
# For mainnet testing: use a wallet with real USDC on Base (chain_id: 8453)
# For testnet testing: use a wallet with testnet USDC on Base Sepolia (chain_id: 84532)
MCP_WEATHER_TEST_PRIVATE_KEY=

# OpenWeatherMap API key for weather endpoint testing
MCP_WEATHER_TEST_WEATHER_API_KEY=
